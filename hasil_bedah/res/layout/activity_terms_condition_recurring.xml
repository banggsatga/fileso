            <TextView ="@dimen/text_size_12sp" ="@id/ninthInstructionPointDetail" ="0.0dip" ="wrap_content" ="@dimen/dimen_12dp" ="Peserta yang telah mengaktifkan fitur layanan pembayaran iuran secara autodebet tidak dapat melakukan pembayaran melalui kanal pembayaran lain." ="@dimen/dimen_12dp" ="inter_word" layout_constraintEnd_toEndOf="@id/tvFirstPage" layout_constraintStart_toEndOf="@id/FirstPagePoint" layout_constraintTop_toBottomOf="@id/eightInstructionPointDetail" style="@style/BpjstkuTextViewPoppins.CharcoalGrey" />
