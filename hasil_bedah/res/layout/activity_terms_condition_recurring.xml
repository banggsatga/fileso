        <androidx.constraintlayout.widget.ConstraintLayout ="@id/constraintLayoutPayment" ="fill_parent" ="wrap_content">
            <TextView ="@dimen/text_size_14sp" ="@id/tvTitleTermsCondition" ="fill_parent" ="wrap_content" ="@dimen/dimen_20dp" ="@dimen/dimen_32dp" ="@dimen/dimen_20dp" =" MOHON MEMBACA DENGAN SEKSAMA SEBELUM MENGGUNAKAN FITUR LAYANAN PEMBAYARAN IURAN SECARA AUTODEBET/OTOMATIS INI" ="center" ="@dimen/dimen_20dp" layout_constraintEnd_toEndOf="parent" layout_constraintStart_toStartOf="parent" layout_constraintTop_toTopOf="parent" style="@style/BpjstkuTextViewPoppins.Black.SemiBold" />
