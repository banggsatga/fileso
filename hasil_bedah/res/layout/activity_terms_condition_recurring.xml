            <TextView ="@dimen/text_size_12sp" ="@id/FirstPagePointDetail" ="0.0dip" ="wrap_content" ="@dimen/dimen_12dp" ="Mendapatkan kemudahan perlindungan Jaminan Sosial Ketenagakerjaan." ="@dimen/dimen_12dp" layout_constraintEnd_toEndOf="@id/tvFirstPage" layout_constraintStart_toEndOf="@id/FirstPagePoint" layout_constraintTop_toBottomOf="@id/tvFirstPage" style="@style/BpjstkuTextViewPoppins.CharcoalGrey" />
