            <TextView ="@dimen/text_size_12sp" ="@id/sevenInstructionPointDetail" ="0.0dip" ="wrap_content" ="@dimen/dimen_12dp" ="Tanggal dilakukan pendebetan adalah tanggal Peserta melakukan pendaftaran pembayaran iuran secara autodebet setiap bulannya, dimulai dari 1 (satu) bulan sejak Peserta melakukan pendaftaran autodebet." ="@dimen/dimen_12dp" layout_constraintEnd_toEndOf="@id/tvFirstPage" layout_constraintStart_toEndOf="@id/FirstPagePoint" layout_constraintTop_toBottomOf="@id/sixthInstructionPointDetail" style="@style/BpjstkuTextViewPoppins.CharcoalGrey" />
            <TextView ="@dimen/text_size_12sp" ="@id/sevenInstructionPointSample" ="wrap_content" ="wrap_content" ="@dimen/dimen_12dp" ="Contoh / Ilustrasi :" layout_constraintStart_toStartOf="@id/sevenInstructionPointDetail" layout_constraintTop_toBottomOf="@id/sevenInstructionPointDetail" style="@style/BpjstkuTextViewPoppins.CharcoalGrey" />
