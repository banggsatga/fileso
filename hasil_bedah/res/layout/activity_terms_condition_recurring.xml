            <TextView ="@dimen/text_size_12sp" ="@id/SecondPagePointDetail" ="0.0dip" ="wrap_content" ="@dimen/dimen_12dp" ="Tidak perlu khawatir kelupaan membayar iuran." ="@dimen/dimen_12dp" layout_constraintEnd_toEndOf="@id/tvFirstPage" layout_constraintStart_toEndOf="@id/FirstPagePoint" layout_constraintTop_toBottomOf="@id/FirstPagePointDetail" style="@style/BpjstkuTextViewPoppins.CharcoalGrey" />
