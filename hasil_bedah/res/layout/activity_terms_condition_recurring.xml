            <TextView ="@dimen/text_size_12sp" ="@id/thirdPagePointDetail" ="0.0dip" ="wrap_content" ="@dimen/dimen_12dp" ="Tidak perlu bepergian ke kanal pembayaran iuran fisik yang memerlukan biaya (bensin,parkir atau angkutan kota, dll)" ="@dimen/dimen_12dp" ="inter_word" layout_constraintEnd_toEndOf="@id/tvFirstPage" layout_constraintStart_toEndOf="@id/FirstPagePoint" layout_constraintTop_toBottomOf="@id/SecondPagePointDetail" style="@style/BpjstkuTextViewPoppins.CharcoalGrey" />
