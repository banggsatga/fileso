            <TextView ="@dimen/text_size_12sp" ="@id/fourthInstructionPointDetail" ="0.0dip" ="wrap_content" ="@dimen/dimen_12dp" ="Peserta harus mendaftarkan alamat email dan nomor handphone yang sama pada akun layanan uang elektronik atau bank miliknya." ="@dimen/dimen_12dp" layout_constraintEnd_toEndOf="@id/tvFirstPage" layout_constraintStart_toEndOf="@id/FirstPagePoint" layout_constraintTop_toBottomOf="@id/thirdInstructionPointDetail" style="@style/BpjstkuTextViewPoppins.CharcoalGrey" />
