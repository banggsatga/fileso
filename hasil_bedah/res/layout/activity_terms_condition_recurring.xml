            <TextView ="@dimen/text_size_14sp" ="@id/tvTermConditionsTitle" ="fill_parent" ="wrap_content" ="@dimen/dimen_20dp" ="@dimen/dimen_32dp" ="@dimen/dimen_20dp" ="Syarat dan Ketentuan fitur layanan pembayaran iuran BPJS Ketenagakerjaan secara autodebet/otomatis :" ="@dimen/dimen_20dp" layout_constraintEnd_toEndOf="parent" layout_constraintStart_toStartOf="parent" layout_constraintTop_toBottomOf="@id/fourthPagePointDetail" style="@style/BpjstkuTextViewPoppins.Black.SemiBold" />
            <TextView ="@dimen/text_size_12sp" ="@id/firstInstructionPoint" ="wrap_content" ="wrap_content" ="@dimen/dimen_12dp" ="1" ="@dimen/dimen_12dp" layout_constraintStart_toStartOf="@id/tvFirstPage" layout_constraintTop_toBottomOf="@id/tvTermConditionsTitle" style="@style/BpjstkuTextViewPoppins.CharcoalGrey" />
            <TextView ="@dimen/text_size_12sp" ="@id/firstInstructionPointDetail" ="0.0dip" ="wrap_content" ="@dimen/dimen_12dp" ="Peserta diminta untuk menginformasikan kepada keluarganya dan rekan-rekannya bahwa telah menjadi Peserta BPJS Ketenagakerjaan, sehingga jika terjadi risiko, diharapkan keluarga dan rekannya dapat menginformasikan kepada BPJS Ketenagakerjaan." ="@dimen/dimen_12dp" ="inter_word" layout_constraintEnd_toEndOf="@id/tvFirstPage" layout_constraintStart_toEndOf="@id/FirstPagePoint" layout_constraintTop_toBottomOf="@id/tvTermConditionsTitle" style="@style/BpjstkuTextViewPoppins.CharcoalGrey" />
            <TextView ="@dimen/text_size_12sp" ="@id/secondInstructionPoint" ="wrap_content" ="wrap_content" ="@dimen/dimen_12dp" ="2" ="@dimen/dimen_12dp" layout_constraintStart_toStartOf="@id/tvFirstPage" layout_constraintTop_toBottomOf="@id/firstInstructionPointDetail" style="@style/BpjstkuTextViewPoppins.CharcoalGrey" />
            <TextView ="@dimen/text_size_12sp" ="@id/secondInstructionPointDetail" ="0.0dip" ="wrap_content" ="@dimen/dimen_12dp" ="Calon Peserta/Peserta memiliki akun layanan uang elektronik atau kartu debit Bank yang telah aktif untuk melakukan transaksi secara online (jika belum harap mengaktifkan atau menghubungi pihak Bank)." ="@dimen/dimen_12dp" ="inter_word" layout_constraintEnd_toEndOf="@id/tvFirstPage" layout_constraintStart_toEndOf="@id/FirstPagePoint" layout_constraintTop_toBottomOf="@id/firstInstructionPointDetail" style="@style/BpjstkuTextViewPoppins.CharcoalGrey" />
            <TextView ="@dimen/text_size_12sp" ="@id/thirdInstructionPoint" ="wrap_content" ="wrap_content" ="@dimen/dimen_12dp" ="3" ="@dimen/dimen_12dp" layout_constraintStart_toStartOf="@id/tvFirstPage" layout_constraintTop_toBottomOf="@id/secondInstructionPointDetail" style="@style/BpjstkuTextViewPoppins.CharcoalGrey" />
            <TextView ="@dimen/text_size_12sp" ="@id/thirdInstructionPointDetail" ="0.0dip" ="wrap_content" ="@dimen/dimen_12dp" ="Peserta bersedia memastikan memiliki saldo yang cukup untuk pembayaran iuran secara autodebet/otomatis sesuai dengan pilihan yang tersedia pada layanan ini." ="@dimen/dimen_12dp" ="inter_word" layout_constraintEnd_toEndOf="@id/tvFirstPage" layout_constraintStart_toEndOf="@id/FirstPagePoint" layout_constraintTop_toBottomOf="@id/secondInstructionPointDetail" style="@style/BpjstkuTextViewPoppins.CharcoalGrey" />
