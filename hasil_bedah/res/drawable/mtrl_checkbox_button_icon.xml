<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item ="true" ="@id/checked" ="@drawable/mtrl_ic_check_mark" />
    <item ="@id/indeterminate" ="@drawable/mtrl_ic_indeterminate" state_indeterminate="true" />
    <item ="false" ="@id/unchecked" ="@android:color/transparent" />
    <transition ="@drawable/mtrl_checkbox_button_icon_checked_unchecked" ="@id/unchecked" ="@id/checked" />
    <transition ="@drawable/mtrl_checkbox_button_icon_unchecked_checked" ="@id/checked" ="@id/unchecked" />
    <transition ="@drawable/mtrl_checkbox_button_icon_indeterminate_unchecked" ="@id/unchecked" ="@id/indeterminate" />
    <transition ="@drawable/mtrl_checkbox_button_icon_unchecked_indeterminate" ="@id/indeterminate" ="@id/unchecked" />
    <transition ="@drawable/mtrl_checkbox_button_icon_indeterminate_checked" ="@id/checked" ="@id/indeterminate" />
    <transition ="@drawable/mtrl_checkbox_button_icon_checked_indeterminate" ="@id/indeterminate" ="@id/checked" />
</animated-selector>