<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item ="true" ="@id/checked" ="@drawable/btn_checkbox_checked_mtrl" />
    <item ="@id/unchecked" ="@drawable/btn_checkbox_unchecked_mtrl" />
    <transition ="@drawable/btn_checkbox_unchecked_to_checked_mtrl_animation" ="@id/checked" ="@id/unchecked" />
    <transition ="@drawable/btn_checkbox_checked_to_unchecked_mtrl_animation" ="@id/unchecked" ="@id/checked" />
</animated-selector>